{
  "name": "wealthlog-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "npm@8.19.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "prisma:generate": "turbo run prisma:generate",
    "prisma:migrate": "turbo run prisma:migrate",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "turbo": "^2.4.4",
    "prettier": "^3.1.0"
  },
  "dependencies": {
    "react-icons": "^5.5.0"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
